{% extends 'base.html.twig' %}

{% block body %}

    {{ form_start(form) }}
    {{ form_errors(form) }}

    <div class="form-group">
        {{ form_label(form.file) }}
        {{ form_errors(form.file) }}
        {{ form_widget(form.file) }}
    </div>

    <div class="form-group">
        {{ form_label(form.response) }}
        {{ form_errors(form.response) }}
        {{ form_widget(form.response) }}
    </div>

    <div>
        {{ form_widget(form.send) }}
    </div>

    {{ form_end(form) }}
    {{ form(form) }}
{% endblock %}
